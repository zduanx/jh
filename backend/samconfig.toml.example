# ============================================================================
# AWS SAM CONFIG EXAMPLE - DO NOT USE DIRECTLY
# ============================================================================
# This file is an EXAMPLE ONLY to show the structure of samconfig.toml
#
# IMPORTANT: samconfig.toml is now AUTO-GENERATED by scripts/generate_samconfig.py
#
# To generate samconfig.toml:
#   1. Configure .sam-config with your stack settings
#   2. Configure .env.local with PROD_VALUE comments for each parameter
#   3. Run: python3 scripts/generate_samconfig.py
#   4. Or run: jpushapi (which auto-generates both template.yaml and samconfig.toml)
#
# DO NOT manually edit samconfig.toml - your changes will be overwritten!
# Instead, update .sam-config and .env.local, then regenerate.
# ============================================================================

version = 0.1
[default]
[default.deploy]
[default.deploy.parameters]
stack_name = "jh-backend-stack"
resolve_s3 = true
region = "us-east-1"
capabilities = "CAPABILITY_IAM"
parameter_overrides = "AllowedEmails=\"YOUR_EMAIL\" AllowedOrigins=\"YOUR_ALLOWED_ORIGINS\" DatabaseUrl=\"YOUR_DATABASE_URL\" GoogleClientId=\"YOUR_GOOGLE_CLIENT_ID\" JWTSecretKey=\"YOUR_JWT_SECRET_KEY\" TestDatabaseUrl=\"YOUR_TEST_DATABASE_URL\""
confirm_changeset = false
